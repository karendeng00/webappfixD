<h1> <%= @user.name%> </h1> 
<p> ID: <%= @user.netid %> </p>

<p> Phone Number: <%= @user.phone %></p>
<p> User Picture: <%= @user.picture %></p>

<p> <%= link_to "Edit", edit_user_path(@user) %> &nbsp; &nbsp;
<%= link_to "Delete", user_path(@user), method: :delete,
                                                    data: {confirm: "Are you sure you want to delete this User?"} %>
