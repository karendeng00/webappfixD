<h1> User List </h1>
<table style="width:100%">
  <tr>
    <th>Name</th>
    <th>Net ID</th> 
    <th>Phone Number</th>
    <th>Picture</th>
    <th>Edit</th>
    <th>Delete</th> 
  </tr>
  
<% @users.each do |user|%>
  <tr>
    <td> <%= link_to user.name, user_path(user) %> </td>
    <td> <%= user.netid %> </td>
    <td> <%= user.phone %> </td>
    <td> <%= user.picture %> </td>
    <td> <%= link_to "Edit", edit_user_path(user) %> </td> 
    <td> <%= link_to "Delete", users_path(user), method: :delete, 
                            data: {confirm: "Are you sure you want to delete this User"} %> </td> 
  </tr>
  <% end %>

</table>